{
  "name": "fetch-notion-page",
  "description": "A tool to recursively fetch all blocks from a Notion page",
  "version": "0.0.1",
  "keywords": [
    "notion",
    "api",
    "blocks",
    "recursive"
  ],
  "author": "Yuku Kotani (y.kotani@dr-ubie.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yukukotani/fetch-notion-page"
  },
  "homepage": "https://github.com/yukukotani/fetch-notion-page",
  "bugs": "https://github.com/yukukotani/fetch-notion-page/issues",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "fetch-notion-page": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint:biome": "biome check .",
    "lint:tsc": "tsc --noEmit",
    "lint:knip": "knip",
    "lint": "concurrently 'npm:lint:*'",
    "fix:biome": "biome check --write --unsafe .",
    "fix:knip": "knip --fix --allow-remove-files",
    "fix": "concurrently 'npm:fix:*'",
    "format": "biome format --write .",
    "prepublishOnly": "bun run build"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@power-assert/runtime": "^0.3.0",
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "knip": "^5.62.0",
    "rollup-plugin-power-assert": "^0.2.0",
    "tsdown": "^0.14.1",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@notionhq/client": "^4.0.2",
    "@praha/byethrow": "^0.6.3"
  },
  "trustedDependencies": [
    "oxc-resolver"
  ]
}
